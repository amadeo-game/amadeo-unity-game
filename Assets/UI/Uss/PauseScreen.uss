    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.25s;
    background-image: url('project://database/Assets/UI/Textures/UI_atlas.psd?fileID=9148746091350738319&guid=68de98d899585b7469e0fb40a0fcbc56&type=3#icon_close');
}

.pause__back-button:hover {
    background-color: rgba(255, 218, 0, 0);
