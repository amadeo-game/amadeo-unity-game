    width: 500px;
    height: 90px;
    -unity-font-style: normal;
    font-size: 39px;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
    background-color: rgba(243, 156, 18, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.25s;
    background-image: url('project://database/Assets/UI/Textures/UI_atlas.psd?fileID=-487671954620856649&guid=68de98d899585b7469e0fb40a0fcbc56&type=3#bt_08');
}

.pause__resume-button:hover {
    background-color: rgba(241, 196, 15, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
