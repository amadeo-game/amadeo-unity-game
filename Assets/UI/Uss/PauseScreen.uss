.timer__container {
    width: 20%;
    height: 100px;
    position: absolute;
    left: 40%;
    background-color: rgba(31, 255, 0, 0);
    top: 2px;
    opacity: 0;
    align-items: center;
    justify-content: center;
    display: none;
}

.pause__screen {
    align-items: center;
    justify-content: center;
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: 1;
}

.pause__background {
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    opacity: 1;
    background-color: rgba(0, 0, 0, 0.24);
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    position: absolute;
    left: 0;
    top: 0;
    -unity-text-outline-color: rgb(0, 0, 0);
}

.pause__container {
    background-color: rgba(41, 128, 185, 0);
    width: 720px;
    height: 620px;
    align-items: center;
    justify-content: center;
    opacity: 1;
    background-image: url('project://database/Assets/UI/Textures/UI_atlas.psd?fileID=4607225936081355050&guid=68de98d899585b7469e0fb40a0fcbc56&type=3#Window_bg_02');
}

.pause__header {
    position: absolute;
